<beans:beans xmlns="http://www.springframework.org/schema/batch"
	xmlns:beans="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/batch
           http://www.springframework.org/schema/batch/spring-batch-3.0.xsd">

	<job id="job1">
		<step id="step1">
			<tasklet>
				<chunk reader="itemReader" processor="itemProcessor" writer="itemWriter" commit-interval="10" />
			</tasklet>
		</step>
	</job>
	
	<job id="job2">
		<step id="step2">
			<tasklet>
				<chunk reader="itemReader2" writer="itemWriter2" commit-interval="10" />
			</tasklet>
		</step>
	</job>
	
	<beans:bean id="itemReader" class="com.sopra.backend.qualitytool.batch.reader.Reader"/>
	<beans:bean id="itemProcessor" class="com.sopra.backend.qualitytool.batch.processor.Processor"/>
	<beans:bean id="itemWriter" class="com.sopra.backend.qualitytool.batch.writer.Writer"/>
	
	<beans:bean id="itemReader2" class="com.sopra.backend.qualitytool.batch.reader.Reader2"/>
<!-- 	<beans:bean id="itemProcessor" class="com.sopra.backend.qualitytool.batch.processor.Processor"/> -->
	<beans:bean id="itemWriter2" class="com.sopra.backend.qualitytool.batch.writer.Writer2"/>

</beans:beans>